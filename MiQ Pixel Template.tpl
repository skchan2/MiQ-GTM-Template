const log = require('logToConsole');
const sendPixel = require('sendPixel');
const encodeUriComponent = require('encodeUriComponent');

const miqTagType = data.miqTagType;

const XandrTag = data.XandrTag;
const XPid = data.XPid;
const XSid = data.XSid;

const DV360Tag = data.DV360Tag;
const DFid = data.DFid;
const DType = data.DType;
const DCat = data.DCat;

const TTDTag = data.TTDTag;
const TAid = data.TAid;
const TPid = data.TPid;

if(miqTagType=="capture"){
  const varArr = data.variableTable1;
  var varText = "";
  var xCap = "";
  var dCap = "";
  var tCap = "";
  
  var orderId = data.orderId;
  if(orderId==null){
    orderId = "";
  }
  var orderValue = data.orderValue;
  if(orderValue==null){
    orderValue = "";
  }
  var a = 1;
  varArr.forEach(function(varItem){
    varText = varItem.variableSyntax;
    if(typeof varItem.variableSyntax == "object"){
      varText = varItem.variableSyntax.toString();
    }
    xCap += "&u" + a + "=" + varItem.variableSyntax;
    dCap += ";u" + a + "=" + varItem.variableSyntax;
    if(a<11){
      tCap += "&td" + a + "=" + varItem.variableSyntax;
    }
    a++;
  });
  var miqUrl = encodeUriComponent("https://pixel.mediaiqdigital.com/pixel?pixel_id=" + XPid + "&uid=${UID}&t=2" + xCap);
}
if(XandrTag){
  var xandrUrl = "https://secure.adnxs.com/px?id=" + XPid + "&seg=" + XSid;
  if(miqTagType=="basic"){
    xandrUrl += "&t=1";
  }else if(miqTagType=="capture"){
    xandrUrl += "&order_id=" + orderId + "&value=" + orderValue + "&redir=" + miqUrl;
  }
  log(xandrUrl);
}
if(DV360Tag){
  var dv360Url = "https://ad.doubleclick.net/ddm/activity/src=" + DFid + ";type=" + DType + ";cat=" + DCat + ";dc_lat=;dc_rdid=;tag_for_child_directed_treatment=;tfua=;npa=;gdpr=${GDPR};gdpr_consent=${GDPR_CONSENT_755}";
  if(miqTagType=="basic"){
    dv360Url += ";ord=1?";
  }else if(miqTagType=="capture"){
    dv360Url += dCap + ";ord=" + orderId + ";cost=" + orderValue + ";qty=1?";
  }
  log(dv360Url);
}
if(TTDTag){
  var ttdUrl = "https://insight.adsrvr.org/track/pxl/?adv=" + TAid + "&ct=0:" + TPid + "&fmt=3";
  if(miqTagType=="capture"){
    ttdUrl += tCap + "&orderid=" + orderId + "&v=" + orderValue;
  }
  log(ttdUrl);
}

// send Pixels when the tag is finished.
if(XandrTag){
  sendPixel(xandrUrl,data.gtmOnSuccess(),data.gtmOnFailure());
}
if(DV360Tag){
  sendPixel(dv360Url,data.gtmOnSuccess(),data.gtmOnFailure());
}
if(TTDTag){
  sendPixel(ttdUrl,data.gtmOnSuccess(),data.gtmOnFailure());
}


